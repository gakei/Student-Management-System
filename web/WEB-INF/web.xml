<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <filter>
        <filter-name>password</filter-name>
        <filter-class>com.WHJ.filter.PasswordFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>password</filter-name>
        <url-pattern>*.jsp</url-pattern>
    </filter-mapping>

    <servlet>
        <!-- 类名 -->
        <servlet-name>InputStuInfo</servlet-name>
        <!-- 所在的包 -->
        <servlet-class>com.WHJ.servlet.InputStuInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InputStuInfo</servlet-name>
        <!-- 访问的网址 -->
        <url-pattern>/InputStuInfo</url-pattern>
    </servlet-mapping>

    <servlet>
        <!-- 类名 -->
        <servlet-name>ShowStudentList</servlet-name>
        <!-- 所在的包 -->
        <servlet-class>com.WHJ.servlet.ShowStudentList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowStudentList</servlet-name>
        <!-- 访问的网址 -->
            <url-pattern>/ShowStudentList</url-pattern>
    </servlet-mapping>



    <servlet>
    <!-- 类名 -->
    <servlet-name>TestAjax</servlet-name>
    <!-- 所在的包 -->
    <servlet-class>com.WHJ.servlet.ShowExamList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestAjax</servlet-name>
        <!-- 访问的网址 -->
        <url-pattern>/TestAjax</url-pattern>
    </servlet-mapping>

    <servlet>
        <!-- 类名 -->
        <servlet-name>EditStudentInfo</servlet-name>
        <!-- 所在的包 -->
        <servlet-class>com.WHJ.servlet.EditStudentInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditStudentInfo</servlet-name>
        <!-- 访问的网址 -->
        <url-pattern>/EditStudentInfo</url-pattern>
    </servlet-mapping>

    <servlet>
        <!-- 类名 -->
        <servlet-name>DeleteStuInfo</servlet-name>
        <!-- 所在的包 -->
        <servlet-class>com.WHJ.servlet.DeleteStuInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteStuInfo</servlet-name>
        <!-- 访问的网址 -->
        <url-pattern>/DeleteStuInfo</url-pattern>
    </servlet-mapping>

    <servlet>
        <!-- 类名 -->
        <servlet-name>Logout</servlet-name>
        <!-- 所在的包 -->
        <servlet-class>com.WHJ.servlet.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <!-- 访问的网址 -->
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>

    <servlet>
        <!-- 类名 -->
        <servlet-name>DormitoryList</servlet-name>
        <!-- 所在的包 -->
        <servlet-class>com.WHJ.servlet.DormitoryList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DormitoryList</servlet-name>
        <!-- 访问的网址 -->
        <url-pattern>/DormitoryList</url-pattern>
    </servlet-mapping>

</web-app>